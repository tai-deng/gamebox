/**index.wxss**/
.container{

}
.menu-usesr{
    width: 100vw;
    height: 580rpx;
    position: relative;
}
.menu-bj{
    position: absolute;
    width: 750rpx;
    height: 580rpx;
    left: 0;
    top: 0;
    z-index: -1;
}
.shool{
    font-size: 24rpx;
    color: #5e5e5e;
    padding: 8rpx 20rpx;
    background-color: rgba(255, 255, 255, 0.8);
    position: absolute;
    left: 0;
    top: 10rpx;
    border-top-right-radius: 30rpx;
    border-bottom-right-radius: 30rpx;
}
.menu-bj > image {
    width: 750rpx;
    height: 580rpx;
    display: block;
}
.menu-list{
    position: relative;
    opacity: .8;
    width: 406rpx;
}
.list-i{
    transform: rotate(45deg);
    font-size: 24rpx;
    color: #888888;
    position: absolute;
    width: 200rpx;
    height: 200rpx;
    background-color: #fff;
    -webkit-tap-highlight-color: transparent;
}
/* .tapStyle {
    background-color: rgba(255, 255, 255, 0.8);
} */
.broadbandInfo{
    width: 750rpx;
    height: 40rpx;
    position: relative;
    top: -30rpx;
    background-color: rgba(255, 255, 255, 0.4);
    font-size: 24rpx;
    color: #888888;
}
.list-mid{
    width: 161rpx;
    height: 161rpx;
    border-radius: 50%;
    font-size: 22rpx;
    color: #ffffff;
    background:linear-gradient(to right,#ff8f00,#ff5100);
    z-index: 5;
}
.list-mid-down{
    width: 161rpx;
    overflow:hidden;
    text-overflow:ellipsis; 
    white-space: nowrap;
}
.list-up{
    top: -168rpx;
    left: 103rpx;
    border-radius: 200rpx 0 0 0;
}
.list-down {
    top: 130rpx;
    left: 103rpx;
    border-radius: 0 0 200rpx 0;
}
.list-right{
    top: -18rpx;
    left: 248rpx;
    border-radius: 0 200rpx 0 0;
}
.list-left{
    top: -19rpx;
    left: -43rpx;
    border-radius: 0 0 0 200rpx;
}
.list-up-v{
    transform: rotate(315deg);
    -webkit-tap-highlight-color: transparent;
}
.list-right-v{
    transform: rotate(315deg);
    width: 30rpx;
}
.list-down-v {
    transform: rotate(315deg);
}
.list-left-v {
    width: 30rpx;
    transform: rotate(315deg);
}

.list-up-price{
    color: #ff5800;
}
.menu-bar{
    font-size: 24rpx;
    color: #000000;
    padding-top: 20rpx;
    padding-bottom: 0 20rpx 20rpx 20rpx;
}
.menu-bar-i{
    flex: 1;
}
.menu-bar-i image {
    width: 48rpx;
    height: 48rpx;
    display: block;
}
.menu-bar-warp{
    width: 91rpx;
    height: 91rpx;
    border-radius: 50%;
    margin-bottom: 5rpx;
}
.recharge{
    background: linear-gradient(to bottom,#63d1ff,#1bbbff);
}
.laundry{
    background: linear-gradient(to bottom,#ffd25c,#ffc11f);
}
.water{
    background: linear-gradient(to bottom,#00e993,#00c97f);
}
.integral{
    background: linear-gradient(to bottom,#ff834a,#ff5000);
}
.upward{
    margin-top: -30rpx;
}
.linber{
    width: 750rpx;
    height: 20rpx;
    background-color: #f1f1f1;
}
.entry-title{
    font-size: 28rpx;
    height: 86rpx;
    color: #ff5800;
}
.entry{
    width: 750rpx;
    height: 80rpx;
    border-top: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;
}
input {
    width: 100%;
    margin-left: 20rpx;
    flex: 1;
    font-size: 28rpx;
}
button{
    margin-top: 50rpx;
    width: 680rpx;
    height: 80rpx;
    border-radius: 40rpx;
    margin-left: 35rpx;
    font-size: 26rpx;
    color: #ffffff;
    background:linear-gradient(to right,#ff8f00,#ff5100);
}

.popup-bg{
    top: 0;
    left: 0;
    position: fixed;
    width: 750rpx;
    height: 100vh;
    background-color: rgba(0, 0, 0, .6);
    z-index:4;
}
.popup-content{
    width: 640rpx;
    border-radius: 10rpx;
    background-color: #fff;
    position: relative;
    padding: 20rpx;
}
.popup-content-title{
    font-size: 28rpx;
    color: #000;
}
.popup-content-cancel{
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 32rpx;
    height: 32rpx;
}
.popup-content-cancel > image {
    width: 32rpx;
    height: 32rpx;
    display: block;
}
.popup-content-tip{
    font-size: 20rpx;
    color: #ff5800;
    margin: 10rpx;
}
.popup-content-bar{
    height: 1rpx;
    background-color: #e5e5e5;
    margin-bottom: 30rpx;
}
.popup-content-item{
    margin-bottom: 20rpx;
}
.popup-content-item-l{
    font-size: 24rpx;
    color: #5e5e5e;
    width: 100rpx;
}
.popup-content-item-r{
    flex: 1;
    height: 70rpx;
    line-height: 70rpx;
    border: 1px solid #e5e5e5;
    border-radius: 5rpx;
    font-size: 28rpx;
}
.picker{
    margin-left: 20rpx;
}
.picker-r{
    margin-right: 10rpx;
}
.picker-r > image {
    width: 32rpx;
    height: 32rpx;
    display: block;
}
.popup-content > button {
    width: 520rpx;
    height: 60rpx;
    margin-left: 40rpx;
    margin-top: 26rpx;
}
.popup-content-tip-b {
    color: red;
}


.popUpB{
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 5;
    background-color: rgba(0, 0, 0, .4);
}
.popUpB-out{
    width: 750rpx;
    flex: 1;
}
.popUpB-content{
    width: 750rpx;
    background-color: #fff;
}
.popUpB-content-item{
    padding: 20rpx 15rpx;
    font-size: 26rpx;
    border-bottom: 1px solid #f1f1f1;
}
.popUpB-title{
    padding: 20rpx;
    font-size: 26rpx;
    color: #ff5800;
    background-color: #f9f7f2;
}
.popUpB-content-l{
    color: #999999;
}
.popUpB-content-l-icon{
    margin-right: 15rpx;
}
.popUpB-content-l-icon image {
    width: 32rpx;
    height: 32rpx;
    display: block;
}
.popUpB-content-r{
    color: #888888;
}
.maincolor{
    color: #ff5800;
}
.popUpB .button{
    width: 680rpx;
    height: 80rpx;
    border-radius: 40rpx;
    font-size: 26rpx;
    color: #ffffff;
    background:linear-gradient(to right,#ff8f00,#ff5100);
}
.popUpB .tag-b {
    margin: 25rpx 35rpx;
}